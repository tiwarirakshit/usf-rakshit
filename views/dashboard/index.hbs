<!-- Page Wrapper -->
<div id="wrapper">

  {{>navbar}}

  <!-- Begin Page Content -->
  <div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Payment Details</h1>

    <!-- DataTales Example -->
    <div class="card shadow mb-4">
      <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Payment Information</h6>
      </div>
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
              <tr>
                <th>S.no.</th>
                <th>Name</th>
                <th>Email</th>
                <th>Phone no.</th>
                <th>Course</th>
                <th>Amount</th>
                <th>orderId</th>
                <th>status</th>
              </tr>
            </thead>
            {{!-- <tfoot>
              <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone no.</th>
                <th>Course</th>
                <th>Amount</th>
              </tr>
            </tfoot> --}}
            <tbody>
              {{#each payment}}
              <tr>
                <td>{{inc @index}}</td>
                <td>{{this.name}}</td>
                <td>{{this.email}}</td>
                <td>{{this.phone}}</td>
                <td>{{this.course}}</td>
                <td>{{this.amount}}</td>
                <td>{{this.orderId}}</td>
                <td>{{this.status}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>

  </div>
  <!-- /.container-fluid -->

</div>
<!-- End of Main Content -->

<!-- Footer -->
{{>footer}}
<!-- End of Footer -->

</div>
<!-- End of Content Wrapper -->

</div>
<!-- End of Page Wrapper -->

{{>logout}}